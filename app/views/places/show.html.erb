<!-- Place details page -->

<h1> <%= @places["name"]%> </h1>

<ul>
<% for post in @posts %>
<h2><%= post["title"]%></h2>
<h4> <%= post["posted_on"]%></h4>
<%= post["description"]%> 
<% end %>
</ul>

<a href="/posts/new?place_id=<%= @places["id"] %>">New Post</a>

<p>
  <a href="/places">Back to All Cities</a>
</p>